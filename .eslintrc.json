{
  "env": {
    "browser": true,
    "jest": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "brace-style": "off",
    "space-before-function-paren": "off",
    "semi": "off"
  }
}
